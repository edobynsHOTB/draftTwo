{
  "name": "adpq2018",
  "version": "0.0.1",
  "description": "Homeowners Toolbox Inc. DBA HOTB Software submission for the California Department of Technology ADPQ in 2018",
  "keywords": [
    "ADPQ",
    "2018"
  ],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/HOTB-Software/adpq-2018"
  },
  "scripts": {
    "start": "docker-compose up --build -d && clear",
    "stop": "docker stop 'server' && docker stop 'db' && clear",
    "build": "docker-compose build --force-rm --no-cache --pull && clear",
    "teardown": "docker rm -f 'server' && docker rm -f 'db' && clear",
    "clean": "npm run clean:docker && npm run clean:db && clear",
    "clean:docker": "docker rmi 'adpq2018_server' && docker rmi 'mongo' && clear",
    "clean:db": "rm -rf ./data"
  },
  "dependencies": {},
  "devDependencies": {},
  "engines": {
    "node": "9.5.0",
    "npm": "5.5.1"
  },
  "os": [
    "any"
  ],
